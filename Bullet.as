package{	import flash.display.MovieClip;	import flash.display.Stage;	import flash.events.*;		public class Bullet extends MovieClip	{		private var radiansBullet:Number				public function Bullet()		{			addEventListener(Event.ENTER_FRAME, eFrame);		}		private function eFrame(event:Event):void		{			radiansBullet = (this.rotation/180)*Math.PI						this.x += 20*Math.cos(radiansBullet);			this.y += 20*Math.sin(radiansBullet);		}	}}